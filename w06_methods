void setup() 
{
  size(400,400);
  background(100);
  looop(width/2,height/2);
}

void looop (int x, int y)
{
fill(0);
circle(x, y, 300);
fill(250);
circle(x, y, 275);
fill(0);
circle(x, y, 250);
fill(250);
circle(x, y, 225);
fill(0);
circle(x, y, 200);
fill(250);
circle(x, y, 175);
fill(0);
circle(x, y, 150);
fill(250);
circle(x, y, 125);
fill(0);
circle(x, y, 100);
fill(250);
circle(x, y, 75);
fill(0);
circle(x, y, 50);
fill(250);
circle(x, y, 25);
}
