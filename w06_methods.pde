void setup() 
{
  size(400,400);
  ooo(width/2,height/2);
}

void ooo (int x, int y)
{
  fill(0);
  circle(x, y, 100);
  fill(250);
  circle(x, y, 90);
  fill(0);
  circle(x, y, 80);
  fill(250);
  circle(x, y, 70);
  fill(0);
  circle(x, y, 60);
  fill(250);
  circle(x, y, 50);
  fill(0);
  circle(x, y, 40);
  fill(250);
  circle(x, y, 30);
  fill(0);
  circle(x, y, 20);
  fill(250);
  circle(x, y, 10);
}
